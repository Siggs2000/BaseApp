<!DOCTYPE html>
<html>
<head>
  <title>NanaConvert</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
    <% flash.each do |name, msg| %>
      <div class="alert alert-<%= name == :notice ? "success" : "error" %>">
        <%= msg  %>
      </div>
    <% end %>

<%= render 'layouts/header' %>

<%= yield %>

</body>
</html>
